
public class FirstClass {
	
	public static void main(String args[]) {
		//variables con operadore
		int num = 7;
		final int num1 = 2;
		float result;
		result = num + num1;
		char letter1 = 'a';
		//constante con operadores
		final double superResul = result / result;
		int a,b,c;
		a = 1;
		b = 2;
		c = 3;
		int resultado = a+b+c;
		
		
		
		
		System.out.println(resultado);
	}

}

