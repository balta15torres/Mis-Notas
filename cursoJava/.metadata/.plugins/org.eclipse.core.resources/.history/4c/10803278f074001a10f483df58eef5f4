import java.util.*;

public class IntroEjemplo1 {

	public static void main(String[] args) {

		Scanner input = new Scanner(System.in);
		
		System.out.println("Introduce tu nombre aquí:");
		String name = input.nextLine();
		
		System.out.println("Introduce también tus apellidos:");
		String surname = input.nextLine();
		
		System.out.println("Introduce también tu fecha de nacimiento:");
		int age = input.nextInt();

		System.out.println("Datos del usuario: " + name + " " + surname + " nacido el:" + age +  " ¡¡Gracias por todo TQM!!");
	}

}
