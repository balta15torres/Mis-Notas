
public class FirstClass {
	
	public static void main(String args[]) {
		
		//variables
		int num = 7;
		final int num1 = 2;
		float result;
		result = num + num1;
		char letter1 = 'a';
	
		//constante
		final double superResul = result / result;
		
		char a,b,c,d;
		a = 'B';
		b = 'a';
		c = 'l';
		d = 't';
		
		System.out.println(a + b);
	}

}

