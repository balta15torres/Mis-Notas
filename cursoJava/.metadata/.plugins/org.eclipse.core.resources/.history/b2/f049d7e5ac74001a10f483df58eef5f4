
public class FirstClass {
	
	public static void main(String args[]) {
		
		//variables
		int num = 7;
		final int num1 = 2;
		float result;
		result = num / num1;
		char letter1 = 'a';
	
		
		
		//constante
		final char letter2 = 'b';
		final double superResul = result/result;
		
		System.out.println(result);
	}

}

