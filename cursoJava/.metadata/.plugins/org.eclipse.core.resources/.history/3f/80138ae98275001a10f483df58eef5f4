import javax.swing.*;

public class AccecsApp {

	public static void main(String[] args) {
		
		String user = "";
		user = JOptionPane.showInputDialog("REGISTRO: \nIntroduce nombre del usuario");
		
		String check = "";
		check = JOptionPane.showInputDialog("REGISTRO: \nIntroduce la contrase単a");
		
		String password = "";
		
		while(check.equals(password) == false) {
			
			password = JOptionPane.showInputDialog("Introduce la contrase単a");
			
			if(check.equals(password) == false) {
				System.out.println("Contrase単a incorrecta, sigue probando");
			}
			
			System.out.println("Contrase単a correcta, Adelante!!!");
			
		}

	}

}
